<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="lifesavings.activities" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="lifesavings.activities.HomeActivity"
            android:label="@string/app_name_edit" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="lifesavings.activities.ExerciseStatsActivity"
            android:label="@string/title_activity_exercise_stats" >
        </activity>
        <activity
            android:name="lifesavings.activities.FoodActivity"
            android:label="@string/title_activity_food" >
        </activity>
        <activity
            android:name="lifesavings.activities.EditProfileActivity"
            android:label="@string/title_activity_profile" >
        </activity>
    </application>

</manifest>
